Anexo la tabla de mysql
nombre de la base de datos personajesws

Create table personajes (
	id Int NOT NULL,
	nombre Varchar(50),
	nacionalidad Varchar(50),
	empresa Varchar(50),
 Primary Key (id)) ENGINE = MyISAM;
 
